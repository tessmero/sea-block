{
  "extension": ["ts"],
  "spec": [
    "tests/**/*.test.ts",
    "eslint-plugin-sb/tests/**/*.test.ts"
  ],
  "require": [
    "ts-node/register",
    "tsconfig-paths/register",
    "./tests/fixtures.ts"
  ],
  "node-option": [
    "experimental-specifier-resolution=node",
    "loader=ts-node/esm"
  ]
}